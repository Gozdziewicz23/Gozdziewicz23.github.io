{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
   "data": {
    "url": "https://raw.githubusercontent.com/Gozdziewicz23/Gozdziewicz23.github.io/main/nasdaq-historical-chart.csv.csv"
 
  },
  "mark": "line",
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "timeUnit": "year",
      "title": "Year",
      "format": "utcyear"
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "NASDAQ Composite value"
    }
  },
  "title": {
    "text": "NASDAQ Composite Index",
    "fontSize": 20,
    "anchor": "start"
  },
  "axis": {
    "grid": true,
    "labelFontSize": 14
  },
  "legend": {
    "title": "",
    "labelFontSize": 14
  },
  "height": 400,
  "width": "container"
}

