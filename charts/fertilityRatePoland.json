{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Cash Benefits solution",
  "title": {
    "text": "Cash Benefits solution",
    "subtitle": "Quick description - removable",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/Gozdziewicz23/Gozdziewicz23.github.io/main/data/fertilityRatePoland.csv",
    "format": {
      "type": "csv"
    }
  },
  "height": 400,
  "width": "container",
  "layer": [
    {
      "mark": {
        "type": "line",
        "point": true,
        "interpolate": "monotone",
        "color": "blueviolet",
        "tooltip": true
      },
      "encoding": {
        "x": {
          "field": "Time",
          "type": "temporal",
          "title": "Year"
        },
        "y": {
          "field": "Value",
          "type": "quantitative",
          "title": "Fertility rate"
        },
        "color": {
          "field": "Country Name",
          "type": "nominal",
          "legend": null
        }
      }
    },
    {
      "mark": {
        "type": "rule",
        "color": "red",
        "size": 3
      },
      "encoding": {
        "x": {
          "datum": {
            "year": 2018
          }
        }
      }
    }
  ]
}
